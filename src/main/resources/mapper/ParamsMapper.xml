<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.diancan.mapper.ParamsMapper">

    <select id="finds" resultType="Params">SELECT * FROM params</select>

    <update id="updateParams" parameterType="Params">
        UPDATE params SET peopleNum=#{peopleNum},meatNum=#{meatNum},cheapNum=#{cheapNum},soupNum=#{soupNum} WHERE id=1
    </update>
</mapper>